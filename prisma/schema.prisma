generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Workhours {
  id             String  @id @default(cuid())
  close_interval Int
  is_day_off     Boolean
  open_interval  Int
  weekday        Int
}

model Events {
  id         String @id @default(cuid())
  begin_at   Int
  end_at     Int
  created_at Int
  updated_at Int
}
